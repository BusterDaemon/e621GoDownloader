    "version": "0.2.0",
    "configurations": [
        {
            "name": "Connect to server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 12345,
            "host": "127.0.0.1",
            "apiVersion": 1
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [
                "--maxPostPages", "2",
                "--pTags", "\"stolas (helluva boss)\"",
                "--scrapPosts", "true"
            ]
        },
        {
            "name": "Launch Package with env",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "env": {
                "E621_PROXY_URL": "socks5://127.0.0.1:2080",
                "E621_OUTPUT_DIRECTORY": "./defOut",
                "E621_MAX_PAGES": "1",
                "E621_WAIT_TIME": "1"
            },
            "args": [
                "--pTags", "\"stolas (helluva boss)\"",
                "--scrapPosts", "true",
                "--booru", "e621",
                "--dbPath", "./defOut/downloads.db"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Hard Time",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "--booru", "rule34",
                "--dbPath", "/home/busterd/Pictures/Eshka/downloaded.db",
                "--out", "/home/busterd/Pictures/Eshka/SLS/",
                "--pTags", "stolas (helluva boss),-blitzo (helluva boss)",
                "--proxy", "socks5://127.0.0.1:2080",
                "--scrapPosts", "true",
                "--wait", "1",
                "--maxPostPages", "4"
            ],
        }
    ]
}